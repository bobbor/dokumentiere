		link(rel="stylesheet",href="css/font-awesome.css",media="all")
		link(rel="stylesheet",href="css/sh.min.css",media="all")
		script(src="js/sh_main.min.js")
		script(src="js/sh_javascript.min.js")
		script(data-main="js/main.js",src="js/libs/require-jquery.js")
	body#item(onload="sh_highlightDocument();")
		#wrapper
			#main
				aside.items
					include partials/header
					include partials/filter
					include partials/items
				include partials/content